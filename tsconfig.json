{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": false,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "outDir": "dist"
  },
  "include": [
    "app/api/**/*.ts",
    "lib/**/*.ts",
    "server.js",
    "middleware.ts"
  ],
  "exclude": [
    "app/chat/**/*",
    "app/agent/**/*",
    "app/workflow/**/*",
    "app/landing/**/*",
    "app/home/**/*",
    "app/payment/**/*",
    "app/settings/**/*",
    "app/team/**/*",
    "app/sandbox/**/*",
    "app/sandbox-demo/**/*",
    "app/demo/**/*",
    "app/embed/**/*",
    "app/auth/**/*",
    "app/auth-sync/**/*",
    "app/auth-redirect.tsx",
    "app/page.tsx",
    "app/layout.tsx"
  ]
}
