# Auth0 Configuration
NEXT_PUBLIC_AUTH0_DOMAIN=your-tenant.auth0.com
NEXT_PUBLIC_AUTH0_CLIENT_ID=your-client-id
NEXT_PUBLIC_AUTH0_AUDIENCE=https://your-api-identifier
NEXT_PUBLIC_AUTH0_REDIRECT_URI=https://your-app-url/callback

# Server Configuration
NODE_ENV=production
PORT=3002

# API Configuration
DEFAULT_API_KEY=your-api-key-here

# Frontend URL
NEXT_PUBLIC_APP_URL=https://your-frontend-url.netlify.app

# Database Configuration
DB_PATH=/app/data

# Plugin Configuration
ALLOWED_ORIGINS=https://example.com,https://another-domain.com
